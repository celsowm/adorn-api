    "sqlite": "^5.1.0",
    "sqlite3": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/sqlite3": "^3.1.11",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
