{
  "name": "simple-pagination-metal-orm",
  "version": "1.0.0",
  "description": "Simple pagination example using metal-orm and SQLite in-memory",
  "type": "module",
  "scripts": {
    "build": "tsc && node ../../dist/cli.cjs build -p tsconfig.json --output .adorn",
    "start": "tsx src/server.ts",
    "dev": "tsx watch src/server.ts"
  },
  "dependencies": {
    "adorn-api": "file:../..",
    "express": "^5.0.0",
